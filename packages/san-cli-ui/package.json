{
    "name": "san-cli-ui",
    "version": "0.0.1",
    "main": "commander.js",
    "license": "MIT",
    "engines": {
        "node": ">=8"
    },
    "scripts": {
        "ui": "cross-env DEBUG=san-cli:* node ../san-cli/index.js ui",
        "build": "cross-env NODE_ENV=production node ../san-cli/index.js build --config san.config.js",
        "prepublish": "npm run build",
        "analyze": "cross-env NODE_ENV=production node ../san-cli/index.js build --config san.config.js --analyze",
        "serve": "cross-env SAN_VAR_APP_GRAPHQL_ENDPOINT=ws://localhost:8333/graphql node ../san-cli/index.js serve --config san.config.js",
        "start": "cross-env DEBUG=san-cli:ui:* SAN_CLI_UI_DEV=true nodemon ../san-cli/index.js ui",
        "prod": "cross-env DEBUG=san-cli:ui:*  SAN_CLI_UI_DEV=false node ../san-cli/index.js ui",
        "dev:san": "node ../san-cli/index.js",
        "dev:san-ui": "cross-env DEBUG=san-cli:* SAN_CLI_UI_DEV=true node ../san-cli/index.js ui",
        "dev:san-init": "node ../san-cli/index.js init"
    },
    "nodemonConfig": {
        "ignore": [
            "client/*",
            "public/*",
            "dist/*"
        ],
        "delay": "2000"
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/ecomfe/san-cli"
    },
    "site": "https://ecomfe.github.io/san-cli",
    "keywords": [
        "san",
        "san.js",
        "webpack",
        "san-ui",
        "san-cli"
    ],
    "dependencies": {
        "@akryum/winattr": "^3.0.0",
        "@jinzhan/open-browser": "^0.0.1",
        "apollo-server-express": "^2.13.0",
        "chalk": "^4.0.0",
        "execa": "^4.0.1",
        "express": "^4.17.1",
        "express-history-api-fallback": "^2.2.1",
        "fs-extra": "^9.0.0",
        "globby": "^11.0.0",
        "graphql-type-json": "^0.3.1",
        "launch-editor": "^2.2.1",
        "lowdb": "^1.0.0",
        "node-ipc": "^9.1.1",
        "node-notifier": "^7.0.0",
        "portfinder": "^1.0.25",
        "san-cli-utils": "^0.1.0",
        "santd": "0.2.7",
        "shortid": "^2.2.15"
    },
    "devDependencies": {
        "animate.css": "^4.1.0",
        "apollo-cache-inmemory": "^1.6.5",
        "apollo-client": "^2.6.8",
        "apollo-link-ws": "^1.0.20",
        "babel-plugin-import": "^1.13.0",
        "cross-env": "^7.0.2",
        "graphql-tag": "^2.10.3",
        "nodemon": "^2.0.4",
        "san": "^3.8.3",
        "san-router": "^1.2.2",
        "santd": "^0.2.5",
        "subscriptions-transport-ws": "^0.9.16",
        "tiny-emitter": "^2.1.0"
    }
}
